---
import BaseLayout from "../layouts/BaseLayout.astro";
import TimeLineElement from "../components/cv/TimeLine.astro";
---

<BaseLayout title="Talks" sideBarActiveItemID="talks">
  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Invited Talks</div>
  </div>

  <div class="time-line-container mb-10">
    <TimeLineElement      
      title="2024"            
    >
     <TimeLineElement
      title="Learning based modelling of human-machine interfaces"
      subtitle="at Healthcare Biorobotics Lab (PI: Liang He), Oxford University, London, UK"
      >
      </TimeLineElement>
      <TimeLineElement
        title="Panelist: Robotic and Digital Twin: Towards Real Health solutions?"
        subtitle="at International Conference on Biomedical Robotics and Biomechatronics, Heidelberg, Germany"
      >      
      </TimeLineElement>
      <TimeLineElement
        title="Learning based modelling of human-machine interfaces"
        subtitle="at Computer Vision for Wearable Robotics (PI: Letizia Gionfrida), King's College London, London, UK"
      >      
      </TimeLineElement>
      <TimeLineElement
        title="Digital Twins for Advanced Back Support Exoskeletons for the Industry"
        subtitle="at Defense Research and Development Organization, 16 - 17 April, Bengaluru, India"
      >      
      </TimeLineElement>
      <TimeLineElement
        title="Wearable Models for Assistive Devices"
        subtitle="at Indian Institute of Technology, Delhi, India"
      >        
      </TimeLineElement>
    </TimeLineElement>    
